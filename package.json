{
  "name": "QA-Core",
  "version": "0.1.0",
  "description": "Helper for QA-Core team",
  "private": true,
  "type": "module",
  "bin": {
    "QA-Core": "./build/index.js"
  },
  "files": [
    "build"
  ],
  "scripts": {
    "build": "tsc && node -e \"require('fs').chmodSync('build/index.js', '755')\"",
