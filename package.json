    "watch": "tsc --watch",
    "inspector": "npx @modelcontextprotocol/inspector build/index.js"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "0.6.0",
    "axios": "^1.13.2",
    "marked": "^17.0.0",
    "mysql2": "^3.15.3",
    "ssh2": "^1.17.0",
    "tunnel-ssh": "^5.2.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  }
}
